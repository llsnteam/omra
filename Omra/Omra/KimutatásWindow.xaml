<Window x:Class="Omra.KimutatásWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="KimutatásWindow" Height="438.248" Width="839.171">
    <Grid Background="{StaticResource hatter}">
        <ComboBox x:Name="cmb_kimutatasTipus" HorizontalAlignment="Left" Margin="648,370,0,0" VerticalAlignment="Top" Width="142" Height="28"/>
        <Button Content="Kimutatás készítése" HorizontalAlignment="Left" Margin="28,370,0,0" VerticalAlignment="Top" Width="129" Height="26" Click="click_kimutatas_keszites" RenderTransformOrigin="0.513,0.721" Style="{StaticResource BigButton}"/>
        <Label Content="Kimutatás típusa:" HorizontalAlignment="Left" Margin="648,341,0,0" VerticalAlignment="Top" Width="111" Height="24"/>
        <DatePicker x:Name="date_tol" HorizontalAlignment="Left" Margin="205,370,0,0" VerticalAlignment="Top" Height="28"/>
        <DatePicker x:Name="date_ig" HorizontalAlignment="Left" Margin="420,370,0,0" VerticalAlignment="Top" Height="28"/>
        <Label Content="-tól" HorizontalAlignment="Left" Margin="343,370,0,0" VerticalAlignment="Top"/>
        <Label Content="-ig" HorizontalAlignment="Left" Margin="555,369,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.061,-0.573" Height="28"/>
        <Canvas x:Name="grafikon" HorizontalAlignment="Left" Height="330" Margin="28,14,0,0" VerticalAlignment="Top" Width="760"/>

    </Grid>
</Window>
